html,body{
    width: 100%;
    height: 100%;
}
*{
    margin: 0;
    padding: 0;
}
ul,li{
    list-style-type: none;
}

li {
    margin-top: 10px;
}

.newImg {
    width: 100px;
    height: 100px;
    border: 1px solid red;
}
.imgList img{
    display: block;
    width: 100px;
    height: 100px;
    margin-bottom: 5px;
}

#img_path {
    display: block;
    width: 100px;
    height: 100px;
    margin-bottom: 5px;
}
.imgList_outer {


}
.imgList_outer .scroll_item {
    width:150px;

}
.imgList_outer > div.scroll_item {
    display:none;
}
.imgList > div.scroll_item {
    border:1px solid gray;
    border-bottom:10px;
}
.imgList > div.scroll_item:first-child .scroll_item_moveUp{
    display:none;
}
.imgList > div.scroll_item:last-child .scroll_item_moveDwn{
    display:none;
}



